{
  "expo": {
    "name": "ml recorder",
    "slug": "ml-recorder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/save.png",
      "resizeMode": "contain",
      "backgroundColor": "#27272A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.laugh.recorder"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/save.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.laugh_detection.ml_recorder"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5389d8fb-acf9-449e-a535-b27fc0693c7d"
      }
    }
  }
}
